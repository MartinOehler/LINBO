####### Install

install_target: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/usr/bin/" || $(MKDIR) "$(INSTALL_ROOT)/usr/bin/"
	-$(INSTALL_FILE) "../../../bin/$(QMAKE_TARGET)" "$(INSTALL_ROOT)/usr/bin/$(QMAKE_TARGET)"
	-strip "$(INSTALL_ROOT)/usr/bin/$(QMAKE_TARGET)"

uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) "$(INSTALL_ROOT)/usr/bin/"


install_phrasebooks: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/usr/phrasebooks/" || $(MKDIR) "$(INSTALL_ROOT)/usr/phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/danish.qph" "$(INSTALL_ROOT)/usr/phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/dutch.qph" "$(INSTALL_ROOT)/usr/phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/finnish.qph" "$(INSTALL_ROOT)/usr/phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/french.qph" "$(INSTALL_ROOT)/usr/phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/german.qph" "$(INSTALL_ROOT)/usr/phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/italian.qph" "$(INSTALL_ROOT)/usr/phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/norwegian.qph" "$(INSTALL_ROOT)/usr/phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/russian.qph" "$(INSTALL_ROOT)/usr/phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/spanish.qph" "$(INSTALL_ROOT)/usr/phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/swedish.qph" "$(INSTALL_ROOT)/usr/phrasebooks/"


uninstall_phrasebooks: 
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/phrasebooks/danish.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/phrasebooks/dutch.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/phrasebooks/finnish.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/phrasebooks/french.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/phrasebooks/german.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/phrasebooks/italian.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/phrasebooks/norwegian.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/phrasebooks/russian.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/phrasebooks/spanish.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/phrasebooks/swedish.qph"
	-$(DEL_DIR) "$(INSTALL_ROOT)/usr/phrasebooks/"


install: install_target install_phrasebooks  

uninstall: uninstall_target uninstall_phrasebooks  


####### Compile

.obj/main.o: main.cpp .ui/options.h
	$(CC) -c $(CFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.ui/options.h: options.ui 
	$(UIC) options.ui -o .ui/options.h

.ui/options.cc: .ui/options.h options.ui options.ui.h 
	$(UIC) options.ui -i options.h -o .ui/options.cc

.obj/options.o: .ui/options.cc .ui/options.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/options.o .ui/options.cc

.moc/moc_options.cpp: $(MOC) .ui/options.h
	$(MOC) .ui/options.h -o .moc/moc_options.cpp

